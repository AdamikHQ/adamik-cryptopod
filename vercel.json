{
  "version": 2,
  "builds": [
    {
      "buildCommand": "pnpm run vercel-build",
      "devCommand": "pnpm run start",
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**", "prisma/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
